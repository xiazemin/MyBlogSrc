<p>&lt;?php
    function func_new_each(&amp;$array){
       $res = array();
       $key = key($array);
       if($key !== null){
           next($array); 
           $res[1] = $res[â€˜valueâ€™] = $array[$key];
           $res[0] = $res[â€˜keyâ€™] = $key;
       }else{
           $res = false;
       }
       return $res;
    }</p>

