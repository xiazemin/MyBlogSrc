func DeleteByQuery4ES(ctx context.Context, index string, query elastic.Query) (int64, error) {
	rsp, err := GetESClient().DeleteByQuery(index).Query(query).Refresh(â€œtrueâ€).Do(ctx)
	if err != nil {
		return 0, err
	}
	return rsp.Deleted, nil
}</p>

