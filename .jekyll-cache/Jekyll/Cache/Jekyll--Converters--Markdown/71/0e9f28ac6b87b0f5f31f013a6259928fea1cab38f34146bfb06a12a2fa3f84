<p>type Tree[T any] struct {
  cmp func(T, T) int
  root *node[T]
}</p>

<p>type node[T any] struct {
  left, right *node[T]
  data T
}</p>

<p>func (bt <em>Tree[T]) find(val T) **node[T] {
  pl := &amp;bt.root
  for *pl != nil {
    switch cmp := bt.cmp(val, (</em>pl).data); {
      case cmp &lt; 0 : pl = &amp;(<em>pl).left
      case cmp &gt; 0 : pl = &amp;(</em>pl).right
    default: return pl
    }
  }
  return pl
}
