<p>package main</p>

<p>import â€œgithub.com/tidwall/gjsonâ€</p>

<p>const json = <code class="language-plaintext highlighter-rouge">{"name":{"first":"Janet","last":"Prichard"},"age":47}</code></p>

<p>func main() {
    value := gjson.Get(json, â€œname.lastâ€)
    println(value.String())
    results := gjson.GetMany(json, â€œname.lastâ€, â€œageâ€)
    for _, result := range results {
        println(result.String())
    }
    value = gjson.GetBytes([]byte(json), â€œname.lastâ€)
    println(value.String())
}</p>

